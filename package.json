{
  "name": "calendar-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.5.3",
    "cra-template-mui-light": "2.0.2",
    "rc-year-calendar": "^1.0.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-scripts": "5.0.0"
  },
  "scripts": {
    "start": "env-cmd -f ./config/.env.dev react-scripts start",
    "build": "env-cmd -f ./config/.env.dev react-scripts build",
    "test": "env-cmd -f ./config/.env.dev react-scripts test",
    "eject": "react-scripts eject",
    "start-staging": "env-cmd -f ./config/.env.staging react-scripts start",
    "start-prod": "env-cmd -f ./config/.env.prod react-scripts start",
    "start-qa": "env-cmd -f ./config/.env.qa react-scripts start",
    "build-staging": "env-cmd -f ./config/.env.staging react-scripts build",
    "build-prod": "env-cmd -f ./config/.env.prod react-scripts build",
    "build-qa": "env-cmd -f ./config/.env.qa react-scripts build",
    "test-staging": "env-cmd -f ./config/.env.staging react-scripts test",
    "test-prod": "env-cmd -f ./config/.env.prod react-scripts test",
    "test-qa": "env-cmd -f ./config/.env.qa react-scripts test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
